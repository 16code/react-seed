{
  "name": "react-seed",
  "version": "0.0.1",
  "description": "React Seed",
  "main": "index.js",
  "author": "16code",
  "license": "MIT",
  "private": false,
  "browserslist": [
    "Safari >= 10",
    "last 1 firefox version",
    "Chrome >= 66",
    "Explorer >= 10"
  ],
  "scripts": {
    "upgrade": "yarn upgrade --latest",
    "postupgrade": "yarn run postinstall",
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development npx webpack-dev-server --hot",
    "build:prod": "cross-env BABEL_ENV=production NODE_ENV=production npx webpack -p --progress --colors --display minimal",
    "preview": "cross-env NODE_ENV=development node server.js",
    "predev": "yarn run copy:vendors:dev",
    "postbuild:prod": "yarn run copy:vendors:prod",
    "postinstall": "node ./node_modules/husky/lib/installer/bin install",
    "postuninstall": "node ./node_modules/husky/lib/installer/bin install",
    "copy:vendors:dev": "cross-env NODE_ENV=development node_modules/.bin/gulp copy:dev --silent",
    "copy:vendors:prod": "cross-env NODE_ENV=production node_modules/.bin/gulp copy:prod --silent"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@welldone-software/why-did-you-render": "^3.5.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "mitt": "^1.2.0",
    "node-vibrant": "^3.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-react-constant-elements": "^7.7.4",
    "@babel/plugin-transform-react-display-name": "^7.7.4",
    "@babel/plugin-transform-react-inline-elements": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "@types/react": "^16.9.17",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-redux-saga": "^1.1.3",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^3.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-jsmin": "^0.1.5",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "husky": "^4.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-short": "^5.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "style-resources-loader": "^1.3.3",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-webpack-plugin": "^1.1.2",
    "terser-webpack-plugin": "^2.3.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/**/*.{less,css}": [
      "node_modules/.bin/stylelint --syntax less --fix",
      "git add"
    ],
    "*.json": [
      "node_modules/.bin/prettier --config ./.prettierrc",
      "git add"
    ]
  }
}
