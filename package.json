{
  "name": "react-seed",
  "version": "0.0.1",
  "description": "React Seed",
  "main": "index.js",
  "author": "16code",
  "license": "MIT",
  "private": false,
  "browserslist": [
    "Safari >= 10",
    "last 1 firefox version",
    "Chrome >= 66",
    "Explorer >= 10"
  ],
  "scripts": {
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development npx webpack-dev-server --hot",
    "build:prod": "cross-env BABEL_ENV=production NODE_ENV=production npx webpack -p --progress --colors --display minimal",
    "preview": "cross-env NODE_ENV=development node server.js",
    "predev": "yarn run copy:vendors:dev",
    "postbuild:prod": "yarn run copy:vendors:prod",
    "copy:vendors:dev": "cross-env NODE_ENV=development node_modules/.bin/gulp copy:dev --silent",
    "copy:vendors:prod": "cross-env NODE_ENV=production node_modules/.bin/gulp copy:prod --silent",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "antd": "^3.20.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "enquire.js": "^2.1.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-media": "^1.9.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.5",
    "spark-md5": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "@types/react": "^16.8.23",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cache-loader": "^4.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "3.1.4",
    "cross-env": "^5.2.0",
    "css-loader": "3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-redux-saga": "^1.0.0",
    "express": "^4.17.1",
    "file-loader": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-inject": "^5.0.2",
    "gulp-jsmin": "^0.1.5",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": ">=1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "postcss-short": "^5.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/**/*.{less,css}": [
      "node_modules/.bin/stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit",
      "post-commit": "git update-index --again"
    }
  }
}
