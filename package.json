{
  "name": "react-seed",
  "version": "0.0.1",
  "description": "React Seed",
  "main": "index.js",
  "author": "16code",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development npx webpack-dev-server --hot",
    "build:prod": "cross-env BABEL_ENV=production NODE_ENV=production npx webpack -p --progress --colors --display minimal",
    "preview": "cross-env NODE_ENV=development node server.js",
    "predev": "yarn run copy:vendors:dev",
    "postbuild:prod": "yarn run copy:vendors:prod",
    "copy:vendors:dev": "cross-env NODE_ENV=development node_modules/.bin/gulp copy:dev --silent",
    "copy:vendors:prod": "cross-env NODE_ENV=production node_modules/.bin/gulp copy:prod --silent",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "@types/react": "^16.8.12",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cache-loader": "^2.0.1",
    "clean-webpack-plugin": "^2.0.1",
    "core-js": "3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-redux-saga": "^1.0.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-inject": "^5.0.2",
    "gulp-jsmin": "^0.1.5",
    "html-webpack-include-assets-plugin": "^1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-prettier": "^1.0.6",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "antd": "^3.16.1",
    "classnames": "^2.2.6",
    "enquire.js": "^2.1.6",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-media": "^1.9.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "precommit": {
    "run": "lint-staged",
    "silent": true
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,jsx}": [
        "node_modules/.bin/eslint --fix",
        "git add"
      ],
      "src/**/*.{less,css}": [
        "node_modules/.bin/stylelint --syntax less --fix",
        "git add"
      ]
    },
    "relative": true,
    "ignore": [
      "**/dist/**",
      "**/doc/**",
      "**/test/**",
      "**/libs/**"
    ]
  }
}
